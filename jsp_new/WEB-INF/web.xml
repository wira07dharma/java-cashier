    <servlet>
        <servlet-name>TransactionCashierHandler</servlet-name>
        <servlet-class>com.dimata.cashierweb.ajax.cashier.TransactionCashierHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FingerHandler</servlet-name>
        <servlet-class>com.dimata.cashierweb.ajax.cashier.FingerHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerificationFingerData</servlet-name>
        <servlet-class>com.dimata.posbo.ajax.VerificationFingerData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CashierMaintenance</servlet-name>
        <servlet-class>com.dimata.cashierweb.ajax.cashier.CashierMaintenance</servlet-class>
