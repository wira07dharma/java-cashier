<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>CashierLoginHandler</servlet-name>
        <servlet-class>com.dimata.cashierweb.ajax.cashier.CashierLoginHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OpenCashierHandler</servlet-name>
        <servlet-class>com.dimata.cashierweb.ajax.cashier.OpenCashierHandler</servlet-class>
