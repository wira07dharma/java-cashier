        <servlet-name>TransactionCashierHandlerNew</servlet-name>
        <servlet-class>com.dimata.cashierweb.ajax.cashier.TransactionCashierHandlerNew</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckService</servlet-name>
        <servlet-class>com.dimata.cashierweb.ajax.CheckService</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>masterdata</servlet-name>
        <servlet-class>com.dimata.microservies.masterdata</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CashierLoginHandler</servlet-name>
        <url-pattern>/CashierLoginHandler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OpenCashierHandler</servlet-name>
        <url-pattern>/OpenCashierHandler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TransactionCashierHandler</servlet-name>
        <url-pattern>/TransactionCashierHandler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FingerHandler</servlet-name>
        <url-pattern>/FingerHandler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerificationFingerData</servlet-name>
        <url-pattern>/verification.php</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CashierMaintenance</servlet-name>
        <url-pattern>/CashierMaintenance</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CashierPackage</servlet-name>
        <url-pattern>/CashierPackage</url-pattern>
    </servlet-mapping>
