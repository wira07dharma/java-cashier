    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>masterdata</servlet-name>
        <url-pattern>/masterdata</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
